   "source": [
    "number = int(input(\"Input a number : \"))\n",
    "\n",
    "sum = 0\n",
    "i = 0\n",
    "while i < number:\n",
    "    if i % 3 == 0 or i % 5 == 0:\n",
    "        sum += i\n",
    "    i += 1\n",
    "\n",
    "print(\"Sum of all the multiples of 3 and 5 below\", number, \": \", sum)"
   ]
  }
