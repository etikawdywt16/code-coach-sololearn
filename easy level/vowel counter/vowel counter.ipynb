   "source": [
    "sentence = input(\"Input a sentence: \")\n",
    "\n",
    "vowel = {\"a\", \"i\", \"u\", \"e\", \"o\"}\n",
    "\n",
    "count = 0\n",
    "for i in sentence.lower():\n",
    "    if i in vowel:\n",
    "        count += 1\n",
    "\n",
    "print(\"Total number of vowel:\", count)"
   ]
  }
