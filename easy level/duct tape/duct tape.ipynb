    "from math import ceil\n",
    "\n",
    "height = int(input(\"Height of the door: \"))\n",
    "width = int(input(\"Width of the door: \"))\n",
    "\n",
    "height_duct = 60 / 12\n",
    "width_duct = 2\n",
    "\n",
    "duct = height_duct * width_duct\n",
    "door = height * width\n",
    "\n",
    "print(\"Total rolls: \", end = \"\")\n",
    "print(ceil(door / duct) * 2)"
   ]
